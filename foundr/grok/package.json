{
  "name": "foundr",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/index.js",
    "docker": "docker run --rm -it -p 3000:3000 -v $(pwd):/app node:18-alpine sh -c 'npm install && npm run dev'",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@clerk/remix": "^3.1.0",
    "@remix-run/cloudflare": "^2.0.0",
    "@remix-run/cloudflare-pages": "^2.0.0",
    "@remix-run/react": "^2.0.0",
    "isbot": "^3.6.8",
    "lucide-react": "^0.294.0",
    "openai": "^4.20.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stripe": "^14.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.0.0",
    "@remix-run/eslint-config": "^2.0.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "eslint": "^8.38.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
